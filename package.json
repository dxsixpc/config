{
  "name": "@zpcscc/configs",
  "version": "3.0.0",
  "description": "项目通用配置",
  "keywords": [
    "commitlint",
    "eslint",
    "prettier",
    "tsconfig",
    "types",
    "react",
    "configs"
  ],
  "homepage": "https://zpcscc.github.io/configs",
  "repository": "https://github.com/zpcscc/configs",
  "license": "MIT",
  "author": {
    "name": "zpcscc",
    "email": "dxsixpc@gmail.com"
  },
  "files": [
    "dist",
    "commitlint-config",
    "eslint-config",
    "prettier-config",
    "tsconfig",
    "types"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "prepublishOnly": "father doctor && pnpm run build && cp -r ./dist/* . && rm -rf dist",
    "postpublish": "git clean -fd",
    "start": "dumi dev"
  },
  "dependencies": {
    "@babel/core": "7.25.2",
    "@babel/plugin-syntax-flow": "7.24.7",
    "@babel/plugin-transform-react-jsx": "7.25.2",
    "@babel/runtime": "7.25.4",
    "@commitlint/cz-commitlint": "19.4.0",
    "@types/node": "22.5.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "commitizen": "4.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-unicorn": "55.0.0",
    "inquirer": "9.3.5",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "prettier-plugin-packagejson": "2.5.1",
    "typescript": "5.5.4"
  },
  "devDependencies": {
    "dumi": "2.4.8",
    "father": "4.5.0",
    "gh-pages": "6.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "styled-components": "6.1.12",
    "tslib": "2.6.3",
    "webpack": "5.94.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
