{
  "name": "@zpcscc/configs",
  "version": "3.0.2",
  "description": "项目通用配置",
  "keywords": [
    "commitlint",
    "eslint",
    "prettier",
    "tsconfig",
    "types",
    "react",
    "configs"
  ],
  "homepage": "https://zpcscc.github.io/configs",
  "repository": "https://github.com/zpcscc/configs",
  "license": "MIT",
  "author": {
    "name": "zpcscc",
    "email": "dxsixpc@gmail.com"
  },
  "files": [
    "dist",
    "commitlint-config",
    "eslint-config",
    "prettier-config",
    "tsconfig",
    "types"
  ],
  "scripts": {
    "build": "father build && mkdir -p dist/tsconfig && cp src/tsconfig/tsconfig.json dist/tsconfig/",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "prepublishOnly": "pnpm run build && cp -r ./dist/* . && rm -rf dist",
    "postpublish": "git clean -fd",
    "start": "dumi dev"
  },
  "dependencies": {
    "@babel/core": "7.25.9",
    "@babel/plugin-syntax-flow": "7.25.9",
    "@babel/plugin-transform-react-jsx": "7.25.9",
    "@babel/runtime": "7.25.9",
    "@commitlint/cz-commitlint": "19.5.0",
    "@types/node": "22.7.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "commitizen": "4.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-unicorn": "56.0.0",
    "inquirer": "9.3.5",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-packagejson": "2.5.3",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "dumi": "2.4.13",
    "father": "4.5.1",
    "gh-pages": "6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "styled-components": "6.1.13",
    "tslib": "2.8.0",
    "webpack": "5.95.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
