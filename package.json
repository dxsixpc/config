{
  "name": "@dxsixpc/configs",
  "version": "1.0.0",
  "description": "项目通用配置",
  "keywords": [
    "commitlint",
    "eslint",
    "prettier",
    "stylelint",
    "tsconfig",
    "types",
    "react",
    "vue"
  ],
  "homepage": "https://dxsixpc.github.io/configs",
  "repository": "https://github.com/dxsixpc/configs",
  "license": "MIT",
  "author": {
    "name": "dxsixpc",
    "email": "dxsixpc@gmail.com"
  },
  "files": [
    "dist",
    "commitlint-config",
    "eslint-config",
    "prettier-config",
    "stylelint-config",
    "tsconfig",
    "types"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "prepare": "husky install",
    "prepublishOnly": "father doctor && pnpm run build && cp -r ./dist/* . && rm -rf dist",
    "postpublish": "git clean -fd",
    "start": "dumi dev"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "commitizen": "^4.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "inquirer": "^8.2.5",
    "postcss": ">=8.2.15 <9.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "stylelint": "^15.4.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "tslib": "^2.4.1",
    "vue-eslint-parser": "^9.1.1"
  },
  "devDependencies": {
    "dumi": "^2.1.19",
    "father": "^4.1.8",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.9",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
